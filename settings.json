{
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "JetBrains Mono Regular",
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.compactFolders": false,
    "workbench.editor.labelFormat": "short",
    "breadcrumbs.enabled": false,
    "files.autoSave": "afterDelay",
    "workbench.activityBar.location": "bottom",
    "workbench.productIconTheme": "bongocat",
    "editor.minimap.enabled": false,
    "editor.fontSize": 14,
    "editor.lineHeight": 1.7,
    "editor.renderLineHighlight": "all",
    "background.editor": {
        "useFront": true,
        "style": {
            "background-size": "25rem"
        },
        "images": ["https://i.kym-cdn.com/photos/images/facebook/002/417/809/a7e"],
        "random": false
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "javascript": false,
        "html": false,
        "css": false
    },
    "terminal.integrated.fontFamily": "monospace",
    "github.copilot.selectedCompletionModel": "",
    "github.copilot.nextEditSuggestions.enabled": true,
    "editor.renderWhitespace": "all",
    "background.enabled": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "workbench.colorTheme": "Default High Contrast",
    "indenticator.color.dark": "#111",
    "where-am-i.textTransfrom": "none",
    "wakatime.apiKey": "paste your API key here"
}
